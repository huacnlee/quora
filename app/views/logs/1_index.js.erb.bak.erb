<% if !@logs.blank? %>
  <% @logs.each do |item| %>
    $(".pagination").before('<%= escape_javascript(render("/logs/log",:log => item)) %>');
  <% end %>
  current_page = "<%= (params[:page] || "1").to_i + 1 %>";
  <% if @logs.count < @per_page %>
  $(".pagination").remove();
  <% end %>
<% else %>
  $(".pagination").remove();
<% end %>